{
  "manifest_version": 3,
  "name": "SURFGUARD",
  "description": "SURFGUARD helps you surf the internet waves safely and securely",
  "version": "1.1",
  "icons": {
    "48": "SurfGuardLogo.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "SurfGuardLogo.png"
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "host_permissions": [
    "*://*/*"
  ]
}
