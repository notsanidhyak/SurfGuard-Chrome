{
    "manifest_version": 2,
    "name": "SurfGuard",
    "description": "Your Guardian Against Phishing: Surf Securely through the Internet Waves",
    "version": "1.1",

    "browser_action": {
        "default_icon": "SurfGuardLogo.png",
        "default_popup": "popup.html"
    },

    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*"
    ]
}